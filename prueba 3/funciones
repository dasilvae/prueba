def agregarpersona(registro,nombre,idjugador,juego,puntajeobtenido):
    for a in registro:
        if a["nombre"] == nombre:
            return
    jugador=[
        "nombre"==nombre,
        "idjugador"==idjugador,
        "juego"==juego,
        "puntajeobtenido"==puntajeobtenido, 
    ]    

    jugador.append(jugador)
